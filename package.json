{
	"name": "@brikcss/merge",
	"version": "0.0.0-development",
	"description": "Utility to perform a deep merge of a list of objects or arrays.",
	"keywords": ["brikcss", "merge", "extend", "assign", "deep merge", "clone"],
	"author": "BrikCSS <https://github.com/brikcss>",
	"contributors": ["The Zimmee <thezimmee@gmail.com>"],
	"homepage": "https://github.com/brikcss/merge",
	"repository": {
		"type": "git",
		"url": "https://github.com/brikcss/merge.git"
	},
	"bugs": {
		"url": "https://github.com/brikcss/merge/issues"
	},
	"main": "./dist/lib/merge.js",
	"module": "./dist/module/merge.js",
	"browser": "./dist/browser/merge.js",
	"umd": "./dist/umd/merge.js",
	"directories": {
		"lib": "./dist/lib"
	},
	"files": ["dist"],
	"publishConfig": {
		"tag": "dev",
		"access": "public"
	},
	"scripts": {
		"start": "npm run watch",
		"pretest": "npm run build",
		"test": "concurrently \"npm run test:lint\" \"npm run test:coverage\"",
		"test:lint":
			"node ./node_modules/.bin/eslint ./src/merge.js && echo \"\n[ok] Linter complete.\n\"",
		"test:unit": "jest --config .jestrc.js",
		"test:coverage":
			"jest --config .jestrc.js --coverage && cat coverage/lcov.info | codacy-coverage && cat coverage/lcov.info | coveralls",
		"prebuild": "rimraf dist",
		"build": "rollup --config=.rolluprc.js",
		"watch":
			"concurrently \"npm run build -- --watch\" \"jest --config .jestrc.js --watchAll\"",
		"semantic-release": "semantic-release"
	},
	"devDependencies": {
		"@brikcss/git-hooks": "^2.1.0",
		"@brikcss/watcher-cli": "^1.0.0",
		"@commitlint/cli": "^7.5.0",
		"babel-core": "6.x",
		"babel-plugin-external-helpers": "6.x",
		"babel-preset-env": "1.x",
		"codacy-coverage": "^3.4.0",
		"concurrently": "^4.1.0",
		"coveralls": "3.x",
		"jest": "^24.0.0",
		"rimraf": "2.x",
		"rollup": "^1.1.2",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-commonjs": "9.x",
		"rollup-plugin-node-resolve": "^4.0.0",
		"rollup-plugin-prettier": "0.x",
		"rollup-plugin-uglify": "^6.0.2",
		"semantic-release": "^15.13.3"
	},
	"dependencies": {}
}
